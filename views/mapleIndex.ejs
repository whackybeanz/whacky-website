                                <h3 class="font-index-subsubheader mb-2">Equipment Guide</h3>
                                <div class="d-flex flex-wrap">
                                    <% sections.beginner.forEach(function(guide) { %>
                                    <div class="page-select col-12 col-sm-6 col-md-4 rounded-lg pl-0 mb-3">
                                        <a href="<%= guide.pageUrl %>" class="stretched-link text-decoration-none">
                                            <div class="w-100 h-100 d-flex align-items-center">
                                                <div class="page-select-img-div d-flex justify-content-center px-2 py-1">
                                                    <% if(icons[guide.iconId]) { %><img src="<%= icons[guide.iconId].imgUrl %>"><% } %>
                                                </div>
                                                <p class="mb-0 px-0 px-xl-2"><%= guide.name %></p>
                                            </div>
                                        </a>
                                    </div>
                                    <% }) %>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-12 mb-4 px-0">
                        <div class="index-section-subheader">
                            <h2 class="font-subheader index-collapse cursor-pointer mb-0 px-3 py-2" data-toggle="collapse" href="#side-extras">
                                <div class="custom-caret d-inline-block">&#9207;</div> Extra Resources
                            </h2>
                        </div>
                        <div class="index-section px-0 collapse show" id="side-extras">
                            <div class="d-flex flex-column px-4 pt-3">
                                <h3 class="font-index-subsubheader mb-2">Calculators</h3>
                                <div class="d-flex flex-wrap mb-3">
                                    <% sections.extras.forEach(function(page) { %>
                                        <% if(page.isCalculator) { %>
                                        <div class="page-select col-12 col-sm-6 col-md-4 rounded-lg pl-0 mb-3">
                                            <a href="<%= page.pageUrl %>" class="stretched-link text-decoration-none">
                                                <div class="w-100 h-100 d-flex align-items-center">
                                                    <div class="page-select-img-div d-flex justify-content-center px-3 py-1">
                                                        <% if(icons[page.iconId]) { %><img src="<%= icons[page.iconId].imgUrl %>"><% } %>
                                                    </div>
                                                    <p class="mb-0 px-0 px-xl-2"><%= page.name %></p>
                                                </div>
                                            </a>
                                        </div>
                                        <% } %>
                                    <% }) %>
                                    <div class="page-select col-12 col-sm-6 col-md-4 rounded-lg pl-0 mb-3">
                                        <a href="/calc/6th-job" class="stretched-link text-decoration-none">
                                            <div class="w-100 h-100 d-flex align-items-center">
                                                <div class="page-select-img-div d-flex justify-content-center px-3 py-1">
                                                    <img src="https://whacky-website.s3-ap-southeast-1.amazonaws.com/images/skills/origin-skill.png" alt="6th Job Icon">
                                                </div>
                                                <p class="mb-0 px-0 px-xl-2">6th Job (Hexa Matrix)</p>
                                            </div>
                                        </a>
                                    </div>
                                </div>

                                <h3 class="font-index-subsubheader mb-2">Info Sheets</h3>
                                <div class="d-flex flex-wrap mb-3">
                                    <% sections.extras.forEach(function(page) { %>
                                        <% if(page.isInfoSheet) { %>
                                        <div class="page-select col-12 col-sm-6 col-md-4 rounded-lg pl-0 mb-3">
                                            <a href="<%= page.pageUrl %>" class="stretched-link text-decoration-none">
                                                <div class="w-100 h-100 d-flex align-items-center">
                                                    <div class="page-select-img-div d-flex justify-content-center px-3 py-1">
                                                        <% if(icons[page.iconId]) { %><img src="<%= icons[page.iconId].imgUrl %>"><% } %>
                                                    </div>
                                                    <p class="mb-0 px-0 px-xl-2"><%= page.name %></p>
                                                </div>
                                            </a>
                                        </div>
                                        <% } %>
                                    <% }) %>
                                </div>

                                <h3 class="font-index-subsubheader mb-2">Fun Corner</h3>
                                <div class="d-flex flex-wrap">
                                    <% sections.extras.forEach(function(page) { %>
                                        <% if(page.isFunCorner) { %>
                                        <div class="page-select col-12 col-sm-6 col-md-4 rounded-lg pl-0 mb-3">
                                            <a href="<%= page.pageUrl %>" class="stretched-link text-decoration-none">
                                                <div class="w-100 h-100 d-flex align-items-center">
                                                    <div class="page-select-img-div d-flex justify-content-center px-3 py-1">
                                                        <% if(icons[page.iconId]) { %><img src="<%= icons[page.iconId].imgUrl %>"><% } %>
                                                    </div>
                                                    <p class="mb-0 px-0 px-xl-2"><%= page.name %></p>
                                                </div>
                                            </a>
                                        </div>
                                        <% } %>
                                    <% }) %>
                                </div>
                            </div>
                        </div>
                    </div>

                    <!-- Horizontal -->
                    <ins class="adsbygoogle ad-space horz-ad rounded-lg mb-3"
                         style="display:block"
                         data-ad-client="ca-pub-9569715514342803"
                         data-ad-slot="9935306861"
                         data-ad-format="auto"
                         data-full-width-responsive="true"></ins>
                    <script>
                        document.addEventListener("DOMContentLoaded", function() {
                         (adsbygoogle = window.adsbygoogle || []).push({});
                        });
                    </script>
                    
                    <div class="col-12 mb-4 px-0">
                        <div class="index-section-subheader">
                            <h2 class="font-subheader index-collapse cursor-pointer mb-0 px-3 py-2" data-toggle="collapse" href="#side-others">
                                <div class="custom-caret d-inline-block">&#9207;</div> Other References
                            </h2>
                        </div>
                        <div class="index-section px-0 collapse show" id="side-others">
                            <div class="d-flex flex-column px-4 pt-3">
                                <div class="d-flex flex-wrap">
                                    <% sections.others.forEach(function(page) { %>
                                        <div class="page-select col-12 col-sm-6 col-md-4 rounded-lg pl-0 mb-3">
                                            <a href="<%= page.pageUrl %>" class="stretched-link text-decoration-none" target="_blank">
                                                <div class="w-100 h-100 d-flex align-items-center">
                                                    <div class="page-select-img-div d-flex justify-content-center px-3 py-1">
                                                        <% if(icons[page.iconId]) { %><img src="<%= icons[page.iconId].imgUrl %>"><% } %>
                                                    </div>
                                                    <p class="mb-0 px-0 px-xl-2"><%= page.name %></p>
                                                </div>
                                            </a>
                                        </div>
                                    <% }) %>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-12 mb-4 px-0">
                        <div class="index-section-subheader">
                            <h2 class="font-subheader index-collapse cursor-pointer mb-0 px-3 py-2" data-toggle="collapse" href="#side-services">
                                <div class="custom-caret d-inline-block">&#9207;</div> Maplestory Services
                            </h2>
                        </div>
                        <div class="index-section px-0 collapse show" id="side-services">
                            <div class="d-flex flex-column px-4 pt-3">
                                <div class="d-flex flex-wrap">
                                    <% sections.quicklinks.forEach(function(page) { %>
                                        <div class="page-select col-12 col-sm-6 col-md-4 rounded-lg pl-0 mb-3">
                                            <a href="<%= page.pageUrl %>" class="stretched-link text-decoration-none" target="_blank">
                                                <div class="w-100 h-100 d-flex align-items-center">
                                                    <% if(icons[page.iconId]) { %><img src="<%= icons[page.iconId].imgUrl %>" class="maple-logos inline-div-img h-100 px-3 py-1"><% } %>
                                                    <p class="mb-0 px-0 px-xl-2"><%= page.name %></p>
                                                </div>
                                            </a>
                                        </div>
                                    <% }) %>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

<%- include("partials/footer") %>