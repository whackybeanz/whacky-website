                                                <option value="1">1</option>
                                                <option value="2">2</option>
                                                <option value="3">3</option>
                                            </select>
                                            <p class="col-4 col-sm-6 mb-0 px-0">
                                                <span class="lang lang-en"><%= content.weeklyName %></span>
                                                <span class="lang lang-tw"><%= content.weeklyNameTw %></span>
                                            </p>
                                        </div>
                                    <% } %>
                                <% }) %>
                            </div>
                        </form>
                    </div>

