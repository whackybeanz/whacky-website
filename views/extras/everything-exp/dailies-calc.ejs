                            <% expContents.symbolContent.forEach((content, index) => { %>
                                <% if(index === 6 || index === 9) { %>
                                    <div class="w-100"></div>
                                <% } %>
                                <div class="single-exp-content calc-daily-quest rounded-sm cursor-pointer mx-1 my-1 position-relative" id="calc-daily-<%= content.iconId %>" data-raw-exp="<%= content.dailyRawEXP %>" data-min-level="<%= content.recMinLevel %>">
                                    <img class="item-square" src="<%= icons[content.iconId].imgUrl %>" data-toggle="tooltip" data-placement="bottom" title="<%= content.name %>">
                                    <svg xmlns="http://www.w3.org/2000/svg" width="32" height="32" fill="currentColor" class="bi bi-check-lg selected-daily text-custom position-absolute d-none" viewBox="0 0 16 16">
                                      <path d="M12.736 3.97a.733.733 0 0 1 1.047 0c.286.289.29.756.01 1.05L7.88 12.01a.733.733 0 0 1-1.065.02L3.217 8.384a.757.757 0 0 1 0-1.06.733.733 0 0 1 1.047 0l3.052 3.093 5.4-6.425a.247.247 0 0 1 .02-.022Z"/>
                                    </svg>
                                </div>
                            <% }) %>
                        </div>
                    </div>

                    <div class="col-12 d-flex flex-wrap px-0 mb-5" id="symbol-weeklies">
                        <div class="col-12 col-sm-4 offset-xl-1 col-xl-3 px-2">
                            <p class="font-subsubheader font-weight-bold mb-0 px-md-3">
                                2) <span class="lang lang-en">Weeklies</span> <span class="lang lang-tw">每周任務</span>
                            </p>
                            <p class="font-small text-custom mb-2 px-3">
                                <span class="lang lang-en">Pick all weeklies you intend to do (level restrictions automatically factored)</span>
                                <span class="lang lang-tw">選擇你要做的所有每周任務 (自動考慮級別限制)</span>
                            </p>
