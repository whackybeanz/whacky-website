                                    <option value="1">Monday</option>
                                    <option value="2">Tuesday</option>
                                    <option value="3">Wednesday</option>
                                    <option value="4">Thursday</option>
                                    <option value="5">Friday</option>
                                    <option value="6">Saturday</option>
                                    <option value="0">Sunday</option>
                                </select>
                            </div>

                            <div class="col-12 d-flex flex-wrap">
                                <% expContents.symbolContent.forEach((content, index) => { %>
                                    <% if(content.hasWeekly) { %>
                                        <div class="col-12 col-sm-6 form-group d-flex justify-content-center justify-content-sm-start align-items-center mb-2 px-0">
                                            <select class="col-4 calc-weekly-quest form-control text-center mr-2" id="calc-weekly-<%= content.iconId %>" data-region="<%= content.iconId %>" data-weekly-name="<%= content.weeklyName %>" data-weekly-name-tw="<%= content.weeklyNameTw %>" data-weekly-raw-exp="<%= content.weeklyRawEXP %>" data-min-level="<%= content.recMinLevel %>">
