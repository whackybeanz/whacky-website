<div class="single-info-page active w-100 align-items-center flex-column px-4 pt-4 px-lg-0 pt-lg-4" id="calculator">
	<div class="info-section w-100 d-flex justify-content-center align-items-center flex-column px-lg-4 pb-3">
		<div class="page-header-box w-100 d-flex justify-content-center align-items-center position-relative mb-2">
			<div class="w-100 flex-grow-1 border-top border-dark"></div>
			<h1 class="font-header w-100 text-center m-0 px-0">Calculator</h1>
			<div class="w-100 flex-grow-1 border-top border-dark"></div>
		</div>
		<div class="w-100 mb-3 justify-content-between">
			<div class="col-12 col-lg-6">
				<div class="position-relative" id="equip-tab-div">
					<img src="/images/equips/equip-tab.png">
					<div class="w-100 h-100 position-absolute" id="equip-grid">
						<div class="equip-slot" id="ring-1-slot"></div>
						<div class="equip-slot" id="ring-2-slot"></div>
						<div class="equip-slot" id="ring-3-slot"></div>
						<div class="equip-slot" id="ring-4-slot"></div>
						<div class="equip-slot" id="pocket-slot"></div>

						<div class="equip-slot" id="pendant-1-slot"></div>
						<div class="equip-slot" id="pendant-2-slot"></div>
						<div class="equip-slot" id="weapon-slot"></div>
						<div class="equip-slot" id="belt-slot"></div>

						<div class="equip-slot" id="hat-slot"></div>
						<div class="equip-slot" id="face-slot"></div>
						<div class="equip-slot" id="eye-slot"></div>
						<div class="equip-slot" id="top-slot"></div>
						<div class="equip-slot" id="bottom-slot"></div>
						<div class="equip-slot" id="shoe-slot"></div>

						<div class="equip-slot" id="earring-slot"></div>
						<div class="equip-slot" id="shoulder-slot"></div>
						<div class="equip-slot" id="gloves-slot"></div>
						<div class="equip-slot" id="android-slot"></div>

						<div class="equip-slot" id="emblem-slot"></div>
						<div class="equip-slot" id="badge-slot"></div>
						<div class="equip-slot" id="medal-slot"></div>
						<div class="equip-slot" id="secondary-slot"></div>
						<div class="equip-slot" id="cape-slot"></div>
						<div class="equip-slot" id="heart-slot"></div>
					</div>
				</div>
			</div>
			<div class="col-12">
				<div id="carousel-fade" class="carousel carousel-fade border rounded-lg" data-ride="carousel" data-interval="false">
					<div class="carousel-job-select w-100 d-flex justify-content-center align-items-center flex-column border-bottom position-relative">
						<div class="carousel-blocker w-100 h-100 d-none justify-content-center align-items-center position-absolute">
							<p class="m-0">Selected class: <span class="selected-job"></span></p>
						</div>
						<div class="w-100 d-flex justify-content-center align-items-center flex-column py-3">
							<p class="mb-1">Select your character's class to begin.</p>
							<div class="d-flex">
								<div class="single-job-choice border border-dark rounded-lg py-1 px-2 mx-2" data-job-select="warrior">Warrior</div>
								<div class="single-job-choice border border-dark rounded-lg py-1 px-2 mx-2" data-job-select="mage">Mage</div>
								<div class="single-job-choice border border-dark rounded-lg py-1 px-2 mx-2" data-job-select="archer">Archer</div>
								<div class="single-job-choice border border-dark rounded-lg py-1 px-2 mx-2" data-job-select="thief">Thief</div>
								<div class="single-job-choice border border-dark rounded-lg py-1 px-2 mx-2" data-job-select="pirate">Pirate</div>
							</div>
						</div>
					</div>
					<div class="carousel-inner position-relative d-none">
						<% Object.keys(allSetItems).forEach(function(equipType, index) { %>
						<div class="equip-select-div carousel-item pt-2 pb-3 px-3 <% if(index ===0) { %>active<% } %>">
							<div class="carousel-caption w-100 p-0">
								<h2 class="font-carousel-header w-100 text-center text-capitalize"><%= equipType %></h2>
								<div class="font-content w-100 d-flex flex-wrap">
								<% allSetItems[equipType].forEach(function(equips) { %>
									<div class="equip-choice equip-<%= equips.jobType %> col-12 col-lg-6 w-100 mb-2 px-2 d-none">
										<div class="single-equip-choice single-equip-<%= equipType %> w-100 d-flex align-items-center border rounded-lg" data-equip-name="<%= equips.name %>" data-set-type="<%= equips.setType %>" data-job-type="<%= equips.jobType %>" data-equip-type="<%= equipType %>" data-choice-img="<%= equips.image %>" data-item-id="<%= equips.id %>">
											<div class="equip-choice-img-div h-100 border-right mr-2 d-flex justify-content-center align-items-center">
												<img src="<%= equips.image %>" class="m-2">
											</div>
											<p class="equip-name m-0 mr-2"><%= equips.name %></p>
										</div>
									</div>
								<% }) %>
								</div>
							</div>
						</div>
						<% }) %>
					</div>
					<div class="carousel-control w-100 border-top d-none">
						<a class="carousel-control-prev w-auto px-2" href="#carousel-fade" role="button" data-slide="prev">
							<i class="fas fa-angle-double-left d-flex align-items-center mr-1"></i>
							<span class="sr-only">Previous</span>
							<p class="font-weight-bold m-0 carousel-control-prev-text">Eye Accessory</p>
						</a>
						<a class="carousel-control-next w-auto px-2" href="#carousel-fade" role="button" data-slide="next">
							<p class="font-weight-bold m-0 carousel-control-next-text">Face Accessory</p>
							<i class="fas fa-angle-double-right d-flex align-items-center ml-1"></i>
							<span class="sr-only">Next</span>
						</a>
					</div>
				</div>
			</div>
