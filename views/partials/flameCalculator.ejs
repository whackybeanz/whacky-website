<div class="single-info-page active w-100 align-items-center flex-column px-4 pt-4 px-lg-0 pt-lg-4" id="flame-calculator">
	<div class="info-section w-100 d-flex justify-content-center align-items-center flex-column mb-3">
		<div class="page-header-box w-100 d-flex justify-content-center align-items-center position-relative border-top border-dark mb-4">
			<h1 class="font-header page-header d-flex align-items-center text-uppercase position-absolute px-3">Settings</h1>
		</div>
		<div class="font-content w-100 d-flex justify-content-center align-items-center flex-column mb-4">
			<form class="font-form d-flex justify-content-center align-items-center flex-column flex-lg-row" id="options-form">
				<div class="col-12 col-lg-6">
					<div class="form-group row d-flex align-items-center">
						<label for="gear-level" class="col-4 col-lg-8 col-form-label font-weight-bold text-left text-lg-right">Equip Level</label>
						<div class="col-8 col-lg-4 d-flex">
							<input class="font-small form-control text-center" type="number" id="gear-level" min=0 max=275 value=0>
						</div>
					</div>
					<div class="form-group row d-flex align-items-center">
						<label for="base-wa-ma" class="col-4 col-lg-8 col-form-label font-weight-bold text-left text-lg-right">Weapon Base WA/MA</label>
						<div class="col-8 col-lg-4 d-flex">
							<input class="font-small form-control text-center" type="number" id="base-wa-ma" min=0 value=0>
						</div>
					</div>
				</div>
				<div class="col-12 col-lg-6">
					<div class="form-group row d-flex align-items-center">
						<label for="equip-level" class="col-4 col-lg-5 col-form-label font-weight-bold text-left text-lg-right">Equip Type <i class="far fa-question-circle" data-toggle="tooltip" data-placement="bottom" title="Zero weapons, CRA boss helmets, Horntail pendants are considered Normal type. See Item Types section for more details."></i></label>
						<div class="col-8 col-lg-7 d-flex justify-content-center">
							<div class="font-small radio-select equip-type-input leftmost-radio active d-flex flex-fill justify-content-center align-items-center px-2" data-equip-type="normal">Normal</div>
							<div class="font-small radio-select equip-type-input d-flex flex-fill justify-content-center align-items-center text-center px-2" data-equip-type="eb">Elite Boss</div>
							<div class="font-small radio-select equip-type-input rightmost-radio d-flex flex-fill justify-content-center align-items-center px-2" data-equip-type="boss">Special</div>
						</div>
					</div>
					<div class="form-group row d-flex align-items-center">
						<label for="equip-level" class="col-4 col-lg-5 col-form-label font-weight-bold text-left text-lg-right">Flame Type</label>
						<div class="col-8 col-lg-7 d-flex justify-content-center">
							<div class="font-small radio-select flame-type-input leftmost-radio active d-flex flex-fill justify-content-center align-items-center px-2" data-flame-type="crimson" data-toggle="tooltip" data-placement="bottom" title="Crimson / Obsidian Resurrection Flames">
								<img src="/images/flames/flame-crimson.png" class="small-img">
							</div>
							<div class="font-small radio-select flame-type-input rightmost-radio d-flex flex-fill justify-content-center align-items-center px-2" data-flame-type="rainbow" data-toggle="tooltip" data-placement="bottom" title="Rainbow / Black Resurrection Flame">
								<img src="/images/flames/flame-rainbow.png" class="small-img">
							</div>
						</div>
					</div>
				</div>
			</form>
		</div>
